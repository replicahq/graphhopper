<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <activeProfiles>
        <activeProfile>gh</activeProfile>
    </activeProfiles>

    <profiles>
        <profile>
            <id>gh</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://repo1.maven.org/maven2</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
                <repository>
                    <id>github-core</id>
                    <name>GH core packages</name>
                    <url>gs://model-maven-repo/snapshot</url>
                </repository>
                <repository>
                    <id>github-mm</id>
                    <name>GH map matching packages</name>
                    <url>https://maven.pkg.github.com/graphhopper/map-matching</url>
                </repository>
            </repositories>
        </profile>
    </profiles>

    <servers>
        <server>
            <id>github-core</id>
            <username>${env.GIT_USER_ID}</username>
            <password>${env.GIT_PERSONAL_ACCESS}</password>
        </server>
        <server>
            <id>github-mm</id>
            <username>${env.GIT_USER_ID}</username>
            <password>${env.GIT_PERSONAL_ACCESS}</password>
        </server>
    </servers>
</settings>
